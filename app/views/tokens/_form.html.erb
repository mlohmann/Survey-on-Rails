<%= form_for(@token) do |f| %>
<% if @token.errors.any? %>
<div class="row">
   <div class="span12"> 
	  
	    <div id="error_explanation">
	      <h2><%= pluralize(@token.errors.count, "error") %> prohibited this question from being saved:</h2>

	      <ul>
	      <% @token.errors.full_messages.each do |msg| %>
	        <li><%= msg %></li>
	      <% end %>
	      </ul>
	    </div>
	  
  </div>
</div>
<% end %>

<div class="row">
   <div class="span12">
	  <div class="field">
	    <%= f.label :title %> 
	  </div>
   </div>
</div>
<div class="row"> 
   	<div class="span3"> 
	   <%= f.text_field :title %> 
   </div>  
   <div class="span3"> 
	  <div class="actions">
	    <%= f.submit(:class => 'btn btn-small btn-success') %>
	  </div> 
   </div>  
</div>
<% end %>
