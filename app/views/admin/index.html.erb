<div class="container">
	<div class="row">
		<div class="span12"><h1>Questions</h1></div>
	</div>
 
	<% @questions.each do |question| %>
	<div class="row">
	    <div class="span9"><%=  question.text  %></div>
	    <div class="span3">	        
	        <%= link_to 'Edit', edit_question_path(question), :class => 'btn btn-small btn-success' %>
	        <%= link_to 'Destroy', question, :method => :delete, :data => { :confirm => 'Are you sure?' }, :class => 'btn btn-small btn-danger' %>
		</div>
	</div>
	<% end %>
 
	<div class="row">
	  <div class="span12"><%= link_to 'New Question', new_question_path, :class => 'btn btn-small btn-primary' %></div> 
	</div>
	
	<div class="row">
		<div class="span12"><h1>Tokens</h1></div>
	</div>
 
	<% @tokens.each do |token| %>
	<div class="row"> 
	    <div class="span9"><%= token.title %></div> 
	    <div class="span3">
			<%= link_to 'Edit', edit_token_path(token), :class => 'btn btn-small btn-success' %>
			<%= link_to 'Destroy', token, :method => :delete, :data => { :confirm => 'Are you sure?' }, :class => 'btn btn-small btn-danger'%></div>
	</div>
	<% end %>

	<div class="row"> 
	  <div class="span12"> <%= link_to 'New Token', new_token_path, :class => 'btn btn-small btn-primary' %></div>
	</div> 
</div>